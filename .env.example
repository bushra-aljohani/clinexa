# OpenAI API Key (for GPT-4 agents)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Hugging Face Token (for TXGemma/MedGemma access)
HF_TOKEN=hf_your-huggingface-token-here

# MongoDB Connection (optional for full RAG features)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGODB_DB_NAME=your_database_name
MONGODB_COLLECTION=your_collection_name

# DeepSeek API Key (optional, for Query Analysis Agent)
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# PubMed E-utilities (optional, free API)
PUBMED_EMAIL=your-email@example.com
PUBMED_API_KEY=your-pubmed-api-key-here

# Model Configuration
DEFAULT_SYNTHESIS_MODEL=google/txgemma-9b-chat
DEFAULT_ANALYSIS_MODEL=gpt-4o-mini
DEFAULT_ROUTING_MODEL=gpt-4o-mini
